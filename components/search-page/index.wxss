.history-title{
  display: flex;
  justify-content: space-between;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.history-report{
  padding: 0 33rpx;
  height: 100vh;

}
.report-item{
  background:rgba(246,247,249,1);
  border-radius:30rpx;
  padding: 17rpx 25rpx;
  margin: 10rpx;
}
.report-box{
  display: flex;
  flex-wrap: wrap;
}
.clearStorage{
  width: 29rpx;
  height: 29rpx;
}
