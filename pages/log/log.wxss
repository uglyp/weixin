.container .login_logo{
  width: 100%;
  height: 550rpx;
  text-align: center;
  padding-top: 30rpx;
}
.container .login_logo image{
  width: 50%;
}
.container button{
  width:90%;
  margin-top:30rpx;
}
.container button:not([size='mini']) {
  width: 90%;
}
.container button.refuse{
  background: var(--lightGrayBg);
  color: var(--gray);
  
}
.container .tip{
  font-size: 26rpx;
  color: #666;
  width: 90%;
  margin: 0 auto;
}