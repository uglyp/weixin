.container scroll-view{
  height: 100%;
}
.product-time{
  width: 100%;
  height: 50rpx;
  background: var(--orangeBg);
}
.product-time.isend{
  background: var(--grayBg);
}
.product-time.isend>view text{
  color:var(--gray);
}
.product-time>view{
  display: flex;
  padding: 0 30rpx;
  height: 50rpx;
}
.product-time>view text{
  color: var(--brown);
  font-size: 24rpx;
  margin: 0 6rpx;
  display: inline-block;
  height: 34rpx;
  line-height: 34rpx;
  margin-top: 8rpx;
}
.product-time>view text.h,
.product-time>view text.m,
.product-time>view text.s{
  color: var(--red);
  border:1rpx solid var(--red);
  min-width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  width: auto;
  padding:0 4rpx;
}

.product-user{
  width: 100%;
  display: flex;
  border-bottom: 20rpx solid var(--grayBg);
}
.product-user .img{
  width: 60rpx;
  height: 60rpx;
  margin: 20rpx;
  margin-top: 36rpx;
}
.product-user .img image{
  width: 100%;
  height: 100%;
}
.product-user .content{
  flex: 1;
}
.product-user .content .name{
  font-size: 30rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.product-user .content>view:last-child{
  font-size: 26rpx;
  margin-bottom: 20rpx;
}
.product-user .content>view:last-child>text:nth-child(2){
  margin-right: 20rpx;
}
.product-user .btns{
  display: flex;
}
.product-user .btns>view,.product-user .btns>button{
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
  margin-top: 40rpx;
  background: none;
  padding:0;
}
.product-user .btns>view text,.product-user .btns>button text{
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  background-repeat: no-repeat;
  background-size: 240rpx 60rpx;
  background-position-x: -60rpx;
}
.product-user .btns>view.favorite_0 text{
  background-position-x: -60rpx;
}
.product-user .btns>view.favorite_1 text{
  background-position-x: 0;
}
.product-user .btns>view:nth-child(2) text{
  background-position-x: -120rpx;
}
product-user .btns>button{
  background: none;
}
.product-user .btns>button text{
  background-position-x: -180rpx;
}
.product-user .btns>button::after{
  border: none;
}

.product-main{
  width: 100%;
  padding: 20rpx 0 0;
}
.product-main .title{
  font-size: 34rpx;
  color:var(--gray);
  line-height: 42rpx;
  padding: 0 20rpx;
  margin-bottom: 20rpx;
  margin-top: 10rpx;
}
.product-main .content{
  padding: 0 20rpx;
  font-size: 30rpx;
  line-height: 38rpx;
  margin-bottom: 30rpx;
  color:var(--gray);
}
.product-main .list{
  
  margin: 0 20rpx;
}
.product-main .list>view{
  background: var(--lightGrayBg);
  padding: 30rpx;
  position: relative;
  margin-bottom: 20rpx;
}
.product-main .list .subtitle{
  font-size: 32rpx;
  line-height: 40rpx;
  margin-bottom: 20rpx;
}
.product-main .list .subcontent{
  font-size: 28rpx;
  line-height: 36rpx;
  margin-bottom: 20rpx;
}
.product-main .list .number{
  display: flex;
}
.product-main .list .number>text{
  font-size: 28rpx;
  margin-bottom: 20rpx;
}
.product-main .list .number>text:nth-child(3){
  margin: 0 20rpx;
}
.product-main .list .price{
  font-size: 28rpx;
}
.product-main .carnum{
  position: absolute;
  display: flex;
  right:20rpx;
  bottom:20rpx;
}
.product-main .carnum>view{
 
  width:44rpx;
  text-align: center;
  height: 44rpx;
  line-height: 40rpx;
  border: 2rpx solid var(--lightGrayBg);
}
.product-main .carnum .num{
  margin: 0 10rpx;
  width: 60rpx;
  
}
.product-main .carnum .reduce {
  border: 2rpx solid var(--orange);
  border-radius: 50%;
  color:var(--orange);
}
.product-main .carnum .reduce.on{
  display: none;
} 
.product-main .carnum .plus{
  border: 2rpx solid var(--orange);
  border-radius:50%;
  background: var(--yellowGradation);
  color:#fff;

}
.product-main .carnum .plus.on{
  border: 2rpx solid var(--darkGrayBg);
  background: var(--darkGrayBg);
}

.product-order{
  width: 100%;
}
.product-order>view{
  margin: 0 20rpx;
  padding: 20rpx 0;
  display: flex;
  border-bottom:1rpx solid var(--grayLine);
}
.product-order>view .index{
  font-size: 28rpx;
  width: 6%;
}
.product-order>view .content{
  flex: 1
}
.product-order>view .content .user{
  display: flex;
}
.product-order>view .content .user image{
  width: 44rpx;
  height: 44rpx;
  margin-right: 20rpx;
}
.product-order>view .content .user text{
  font-size: 28rpx;
   margin-right: 20rpx;
}
.product-order>view .content .num text{
  font-size: 30rpx;
  margin-right: 20rpx;
}
.product-order .moreOrAll{
  font-size: 28rpx;
  color:var(--gray);
  background: var(--lightGrayBg);
  border:none;
}
.product-order .moreOrAll>view{
  width: 100%;
  text-align: center;
}
.product-myorder{
  width: 100%;
}
.product-myorder .subHead{
  width: 100%;
  display: flex;
}
.product-myorder .subHead>view{
  flex: 1;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
}
.product-myorder .subHead>view image{
  width: 36rpx;
  margin-right: 20rpx;
}
.product-myorder .subHead>view text{
  height: 50rpx;
  line-height: 50rpx;
  display: inline-block;
}
.product-myorder .list{
  padding: 20rpx;
}
.product-myorder .list>view{
  padding: 20rpx;
  background: var(--lightOrangeBg);
  display: flex;
}
.product-myorder .list>view .index{
  font-size: 28rpx;
  width: 6%;
}
.product-myorder .list>view  .content{
  flex: 1
}
.product-myorder .list>view  .content .user{
  display: flex;
}
.product-myorder .list>view  .content .user image{
  width: 44rpx;
  height: 44rpx;
  margin-right: 20rpx;
}
.product-myorder .list>view  .content .user text{
  font-size: 28rpx;
   margin-right: 20rpx;
}
.product-myorder .list>view  .content .num text{
  font-size: 30rpx;
  margin-right: 20rpx;
}
.product-myorder .list>view  .content .price{
  text-align: right;
  color:var(--red);
  font-size: 26rpx;
  
}
.product-myorder .list>view  .content .price text{
  margin-left: 20rpx;
}

.product-myorder .list>view  .content view button{
  height: 46rpx;
  line-height: 46rpx;
  border:1rpx solid var(--brown);
  color:var(--brown);
  font-size: 24rpx;
  border-radius: 4rpx;
  width: 170rpx;
  padding: 0;
  background: none;
  float: right;
  margin-top:20rpx;
}
.product-myorder .list>view  .content view button.status_2{
  border:1rpx solid var(--lightGray);
  color:var(--lightGray);
}
.product-myorder .list>view  .content view button.status_3{
  border:1rpx solid var(--lightGray);
  color:var(--lightGray);
}
.product-myorder .list>view  .content view button::after{
  border:none;
  border-radius: 0;
}
.product-button{
  position: fixed;
  width: 100%;
  background: #fff;
  bottom:0rpx;
  padding: 20rpx 0;
}
.product-button button{
  width: 90%;
  text-align: center;
  color: #fff;
  background: var(--yellowGradation);
  font-size: 34rpx;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0;
}
.product-button button:not([size='mini']){
  width: 90%;
}
.product-button .orderBtn{
  display: flex;
  padding: 0 5%;
  height: 90rpx;
}
.product-button .orderBtn>view{
  height: 90rpx;
  line-height: 90rpx;
}
.product-button .orderBtn>view:first-child{
  background: #fff;
  border:2rpx solid var(--orange);
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  color: var(--orange);
  border-radius: 10rpx 0 0 10rpx;
  display: flex;
  justify-content: center;
}
.product-button .orderBtn>view:last-child{
  background: var(--yellowGradation);
  border:2rpx solid var(--orange);
  flex: 2;
  text-align: center;
  font-size: 34rpx;
  color:#fff;
  border-radius: 0 10rpx 10rpx 0;
}
.product-button .orderBtn>view:last-child.isend{
  background: #C7C7C7;
  border:2rpx solid #C7C7C7;
}
.product-button .mySelfBtn{
  margin: 0 5%;
  height: 90rpx;
  display: flex;
  border:2rpx solid var(--orange);
  font-size: 24rpx;
  color:var(--orange);
  border-radius: 10rpx;
  margin-bottom:10rpx; 
}
.product-button .mySelfBtn>view{
  flex: 1;
  text-align: center;
}
.product-button .mySelfBtn>view>view:first-child{
  padding-top: 10rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 36rpx;
}
.product-button .mySelfBtn>view>view>image{
  width: 30rpx;
}

.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
/*对话框 */
.commodity_attr_box {
  
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #FCFCFC;
}
.commodity_attr_box>view{
  margin: 0 20rpx;
  border-bottom: 1rpx solid #EDEDED;
  text-align: center;
  font-size: 32rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.commodity_attr_box>view:last-child,.commodity_attr_box>view:nth-child(3){
  border-bottom: none;
}
.commodity_attr_box>view:last-child{
  border-top: 14rpx solid #EDEDED;
  margin: 0;
}
.product-tips{
  padding: 0 20rpx 20rpx;
  font-size: 24rpx;
}

.canvas{width: 750px; height: 1298px;}
.poster-pop{width:450rpx;height:714rpx;position:fixed;left:50%;transform:translateX(-50%);z-index:99;top:50%;margin-top:-357rpx;}
.poster-pop image{width:100%;height:100%;display:block;}
.poster-pop .close{width:50rpx;height:50rpx;position:fixed;right:0;top:0rpx;display:block;font-size: 48rpx;}
.poster-pop .save-poster{background:var(--orange);font-size:：22rpx;color:#fff;text-align:center;height:76rpx;line-height:76rpx;width:100%;}
.mask{position:fixed;top:0;left:0;right:0;bottom:0;background-color:rgba(0,0,0,0.6);z-index:9;}