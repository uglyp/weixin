.page-box {
    height: 100%;
    box-sizing: border-box;
}

.page-top {
    padding: 0 33rpx;
    background: #fff;
    padding-bottom: 40rpx;
    display: flex;
}

.index-search {
    background: #f7f7f9;
    text-align: center;
    height: 74rpx;
    flex: 1;
}
.search-text{
    font-size:32rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(51,51,51,1);
    line-height:74rpx;
    margin-left: 24rpx;
}
.history-title{
    display: flex;
    justify-content: space-between;
    font-size:28rpx;
    font-family:PingFang SC;
    font-weight:bold;
    color:rgba(51,51,51,1);
}
.history-report{
    padding: 0 33rpx;

}
.report-item{
    background:rgba(246,247,249,1);
    border-radius:30rpx;
    padding: 17rpx 25rpx;
    margin: 10rpx;
}
.report-box{
    display: flex;
    flex-wrap: wrap;
}