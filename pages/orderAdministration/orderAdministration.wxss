.triangle{
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 0 0 26rpx 33rpx;
    border-color: transparent transparent #FB9A2B transparent;
    position: absolute;
    bottom: 0;
    right: 0;
}

.dialog{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(25, 25, 25, 0.8);
    overflow: hidden;
    z-index: 1000;
}
.dialog-content{
    width:692rpx;
    position: relative;
    margin: 0 auto;
    top: 40%; /*偏移*/
    transform: translateY(-50%);
    z-index: 100;
}
.content-info-remainder{
    overflow: hidden;
    height: 563rpx;
    border-radius:10rpx;
}

