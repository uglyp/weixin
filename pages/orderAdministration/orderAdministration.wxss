.select_box {
    background-color: #eee;
    padding: 0 30rpx;
    width: 90%;
    position: absolute;
    top: 84rpx;
    z-index: 10;
    overflow: hidden;
    animation: myfirst 0.5s;
    left: -2rpx;
}
 
@keyframes myfirst {
    from {
        height: 0rpx;
    }
 
    to {
        height: 200rpx;
    }
}
 
.select_one {
    height: 60rpx;
